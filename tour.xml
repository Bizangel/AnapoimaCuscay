<krpano version="1.20.7" title="Virtual Tour">
	<layer name="debug" val="1"/>
	
	<include url="skin/vtourskin.xml" if="0"/>
	
	<include url="vuho_overlay.xml" if="1"/>
	<include url="map.xml" if="1" />
	<include url="vtour_override.xml" />
	
	
	<!-- Debug purposes -->
	<action name="print">
		showlog();
		trace(" ",%1);
	</action>

	<events onkeydown="debugaction();" if="1"/>
	
	<action name="debugaction" if="1" >
		
		if(keycode == 70, <!-- F KEY -->
			toggle_mapmenu();
		);
		
		if(keycode == 49, <!-- 1 KEY -->
			switchFloor(1);
		);
		if(keycode == 50, <!-- 2 KEY -->
			switchFloor(2);
		);
		if(keycode == 72, <!-- H KEY -->
			switchHouse(2f3h);
			print(2 Floors 3 Rooms);
		);
		
		if(keycode == 74, <!-- J KEY -->
			switchHouse(2f2h);
			print(2 Floors 2 Rooms);
		);
		
		if(keycode == 75, <!-- K KEY -->
			switchHouse(1f3h);
			print(1 Floors 3 Rooms);
		);
		if (keycode == 76, <!-- L KEY -->
			switchHouse(1f2h);
			print(1 Floors 2 Rooms);
		);
		
	</action>
	
